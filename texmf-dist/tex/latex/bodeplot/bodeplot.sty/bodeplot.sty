              \str_if_eq:VnT \l_imag_tl {+} { \tl_set:Nn \l_imag_tl {1} }
              \str_if_eq:VnT \l_imag_tl {-} { \tl_set:Nn \l_imag_tl {-1} }
            }
            {
              \tl_set:Nn \l_real_tl {0}
              \tl_set:Nn \l_imag_tl {0}
            }
        }
    }
    {%
      \tl_set_eq:NN \l_real_tl \l_tmpa_tl
