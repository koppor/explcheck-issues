                \xdef\bp@num@im{\bp@num@im+(\numcoeff*(\n@pow{-1}{(\currentdegree-1)/2})*%
                  (\n@pow{t}{\currentdegree}))}
              \else
                \xdef\bp@num@re{\bp@num@re+(\numcoeff*(\n@pow{-1}{(\currentdegree)/2})*%
                  (\n@pow{t}{\currentdegree}))}
              \fi
            \fi
          \fi
        }
