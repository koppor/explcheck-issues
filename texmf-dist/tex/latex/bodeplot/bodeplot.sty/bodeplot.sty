                \xdef\bp@den@im{\bp@den@im+(\dencoeff*(\n@pow{-1}{(\currentdegree-1)/2})*%
                  (\n@pow{t}{\currentdegree}))}
              \else
                \xdef\bp@den@re{\bp@den@re+(\dencoeff*(\n@pow{-1}{(\currentdegree)/2})*%
                  (\n@pow{t}{\currentdegree}))}
              \fi
