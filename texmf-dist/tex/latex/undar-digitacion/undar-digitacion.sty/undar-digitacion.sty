\def\splitPorCaracter#1{\threewords#1\relax}
\def\threewords#1/#2/#3\relax{
    \refstepcounter{contnotas}
    \tikzmark{\arabic{contnotas}}\notaParaMusixtex{#1}{\letraA{#2}}{#3}{\letraB{#2}}\relax
}

