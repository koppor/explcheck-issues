\NewDocumentCommand \addtotheoremhook { O{allthms} m +m }
  { % #1 = envname, #2 = hook, #3 = code
    \__hook_if_declared:nTF { keytheorems/allthms/#2 }
      {
        \hook_gput_code:nnn { keytheorems/#1/#2 } { . } { #3 }
