      {
        \addtotheoremhook [ #1 ] { prehead } { #2 }
      }
    \ProvideDocumentCommand { \addtotheorempostheadhook } { O{allthms} +m }
      {
        \addtotheoremhook [ #1 ] { posthead } { #2 }
      }
    \ProvideDocumentCommand { \addtotheoremprefoothook } { O{allthms} +m }
