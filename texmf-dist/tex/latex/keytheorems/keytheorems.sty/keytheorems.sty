                    right~skip=\l_keythms_tcbthmbox_rightmargin_dim,
                    overlay~unbroken={
                      \draw[line~width=\l_keythms_tcbthmbox_thickness_dim]
                        (title.south~west)
                        --
                        (title.south~east);
                      \draw[line~width=\l_keythms_tcbthmbox_thickness_dim]
                        (frame.north~west)
                        |-
                        ([xshift=10mm]frame.south~west);
                      },
                    overlay~first={
                      \draw[line~width=\l_keythms_tcbthmbox_thickness_dim]
                        (title.south~west)
                        --
                        (title.south~east);
                      \draw[line~width=\l_keythms_tcbthmbox_thickness_dim]
                        (frame.north~west)
                        --
                        (frame.south~west);
                      },
