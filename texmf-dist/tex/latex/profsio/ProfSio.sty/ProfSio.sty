		}%
		%les calculs jusqu'au dernier chiffre
		\foreach \i in {0,...,\tmplastdigit}{%
			\foreach \j in {0,...,\tmplastdigit}{%
				\IfStrEqCase{#3}{%+ par défaut
					{+}{\xdef\tmpcalcbase{\xinteval{\i+\j}}}%
					{*}{\xdef\tmpcalcbase{\xinteval{\i*\j}}}%
					{×}{\xdef\tmpcalcbase{\xinteval{\i*\j}}}%
