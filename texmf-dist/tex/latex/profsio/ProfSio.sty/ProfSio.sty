local function toBinary(x,y)
	y = y or math.max(1, select(2, math.frexp(x)))
	local res = {}
	for i = y, 1, -1 do
		res[i] = math.fmod(x, 2)
		x = math.floor((x - res[i]) / 2)
	end
	return res
end

