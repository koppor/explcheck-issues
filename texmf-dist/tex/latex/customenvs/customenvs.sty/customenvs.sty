					\fi
				\fi
			}%
		\end{scope}
		\xintifboolexpr{\numcasNS >=1 'and' \numcasNS <= \nbcasesNS }%
			{%
				\begin{scope}
					%\clip (-\margeNS,-\margeNS) rectangle++ (\largeurbandeNS+2*\margeNS,\hauteurNS+2*\margeNS);
