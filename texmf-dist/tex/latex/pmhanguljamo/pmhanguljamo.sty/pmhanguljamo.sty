%%%% usepmfont macro

\cs_if_exist:NTF \hangulfontspec
{
	\cs_set_eq:NN \pm_hangulfontspec \hangulfontspec
}
{
	\cs_set_eq:NN \pm_hangulfontspec \fontspec
